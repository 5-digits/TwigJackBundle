{
    "name": "boekkooi/twig-jack-bundle",
    "type": "symfony-bundle",
    "keywords": [ "twig", "symfony", "defer", "inheritance" ],
    "description": "Handy additional features for Twig within symfony 2",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.1",
        "symfony/twig-bundle": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Warnar Boekkooi",
            "email": "warnar@boekkooi.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Boekkooi\\Bundle\\TwigJackBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Boekkooi\\Bundle\\TwigJackBundle\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
